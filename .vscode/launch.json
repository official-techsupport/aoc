    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Pypy: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "'c:/program files/pypy/pypy.exe' ${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}
